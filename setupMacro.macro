st=start %1

l=dir
c=cls
x=exit
ex=explorer %cd%
np="%PROGRAMFILES%\Notepad++\notepad++.exe" $*
st=start &*
t=cd /D "%SystemDrive%\Temp\"
re=refreshenv

g=git $*
gs=git status
gc=git clone $*
